{
  "compilerOptions": {
    "moduleResolution": "node",
    "moduleDetection": "force",
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "js",
    "baseUrl": "./",
    "rootDir": ".",
    "paths": {
      "@fluid/*": [
        "./src/*"
      ],
      "@fluidcore/*": [
        "./src/core/*"
      ],
      "@fluidlib/*": [
        "./src/lib/*"
      ],
    }
  },
  "exclude": [
    "temp"
  ]
}